package com;

public class Bank {
	private int accountnumber;
	private double bal;
	private String name;

	Bank(double amt) {
		bal = amt;
	}

//	Getter method
	public void displaybal() {
		System.out.println(bal);
	}

//	Setter Method
	public void desposite(double amt) {
		bal+=amt;
	}
}
