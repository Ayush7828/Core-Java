package com;

public class Person {
	private String name;
	private int age;
	private int pid;

	public String getName() {
		return name;
	}

	public void setName(String na) {
		name = na;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int a) {
		age = a;
	}

	public int getPid() {
		return pid;
	}

	public void setPid(int p) {
		pid = p;
	}

}
