package com;

public class RectangleArea {
	private int length;
	private int breadth;

	public void setlength(int l) {

		if (length > 0) {
			length = l;
		} else {
			length = 0;
		}

	}

	public int getlength() {
		return length;
	}

	public void breadth(int b) {
		if (breadth > 0)
			breadth = b;

		else
			breadth = 0;

	}

	public int getbreadth() {
		return breadth;
	}

}

class Test {
	public static void main(String[] args) {
		RectangleArea obj = new RectangleArea();
		obj.setlength(-45);
		obj.breadth(-78);

		System.out.println("Length = " + obj.getlength());

		System.out.println("Breadth = " + obj.getbreadth());

	}
}